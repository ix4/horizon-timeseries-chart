{
  "name": "horizon-timeseries-chart",
  "version": "1.6.2",
  "description": "A chart to represent time-series data using multiple vertically layered horizon area plots",
  "unpkg": "dist/horizon-timeseries-chart.min.js",
  "jsdelivr": "dist/horizon-timeseries-chart.min.js",
  "main": "dist/horizon-timeseries-chart.common.js",
  "module": "dist/horizon-timeseries-chart.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/horizon-timeseries-chart.git"
  },
  "keywords": [
    "d3",
    "d3-module",
    "horizon",
    "timeseries",
    "chart",
    "svg"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/horizon-timeseries-chart/issues"
  },
  "homepage": "https://github.com/vasturiano/horizon-timeseries-chart",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c",
    "dev": "rollup -c -w",
    "minify": "terser dist/horizon-timeseries-chart.js -o dist/horizon-timeseries-chart.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepare": "npm run build && npm run minify && npm run build-module"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "accessor-fn": "^1.2.2",
    "d3-array": "^2.4.0",
    "d3-axis": "^1.0.12",
    "d3-horizon": "^1.9.11",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "d3-shape": "^1.3.7",
    "d3-time": "^1.1.0",
    "d3-time-format": "^2.2.3",
    "d3-transition": "^1.3.2",
    "d3-zoom": "^1.8.3",
    "index-array-by": "^1.2.9",
    "kapsule": "^1.12.2",
    "lodash.memoize": "^4.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^2.4.1",
    "terser": "^4.6.6"
  }
}
